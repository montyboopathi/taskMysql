<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>MyServlet</servlet-name>
		<servlet-class>com.kmbtask.MyServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DeleteService</servlet-name>
		<display-name>DeleteService</display-name>
		<description></description>
		<servlet-class>com.kmbtask.DeleteService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ListService</servlet-name>
		<display-name>ListService</display-name>
		<description></description>
		<servlet-class>com.kmbtask.ListService</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyServlet</servlet-name>
		<url-pattern>/MyServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DeleteService</servlet-name>
		<url-pattern>/DeleteService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ListService</servlet-name>
		<url-pattern>/ListService</url-pattern>
	</servlet-mapping>
	<listener>
		<listener-class>com.kmbtask.AppContextListener</listener-class>
	</listener>
</web-app>
